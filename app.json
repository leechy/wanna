{
  "expo": {
    "name": "Wanna Wanna",
    "slug": "wanna-wanna",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "wannawanna",
    "newArchEnabled": true,
    "githubUrl": "https://github.com/leechy/wanna",
    "platforms": ["ios", "android", "web"],
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#f2f2f2",
    "primaryColor": "#3a1fc1",
    "icon": "./assets/images/icon.png",
    "notification": {
      "icon": "./assets/icon-notifications.png",
      "color": "#3a1fc1",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Wanna Wanna"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wanna-wanna.app",
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      },
      "splash": {
        "backgroundColor": "#F2F2F2",
        "dark": {
          "backgroundColor": "#000000"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "monochromeImage": "./assets/icon-mono.png"
      },
      "splash": {
        "backgroundColor": "#F2F2F2",
        "dark": {
          "backgroundColor": "#000000"
        }
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/GreatVibes-Regular.ttf"]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep public class com.horcrux.svg.** {*;}"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1555217c-4179-4b6b-8705-2b9b92556351"
      }
    }
  }
}
