{
  "expo": {
    "name": "Wanna Wanna",
    "slug": "wanna-wanna",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "wannawanna",
    "newArchEnabled": true,
    "githubUrl": "https://github.com/leechy/wanna",
    "platforms": ["ios", "android", "web"],
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#f2f2f2",
    "primaryColor": "#3a1fc1",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/icon-notifications.png",
      "color": "#3a1fc1",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Wanna Wanna"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wanna-wanna.app",
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-adaptive.png",
        "backgroundImage": "./assets/icon-adaptive-bg.png",
        "backgroundColor": "#ffd98d",
        "monochromeImage": "./assets/icon-mono.png"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.wannawanna.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#f2f2f2",
          "image": "./assets/icon-adaptive.png",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/icon-adaptive-dark.png"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/GreatVibes-Regular.ttf"]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep public class com.horcrux.svg.** {*;}"
          }
        }
      ],
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "colorAccent": {
                "light": "#3a1fc1",
                "dark": "#ffd700"
              },
              "textColor": {
                "light": "#3a1fc1",
                "dark": "#ffd700"
              },
              "colorControlActivated": {
                "light": "#3a1fc1",
                "dark": "#ffd700"
              }
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1555217c-4179-4b6b-8705-2b9b92556351"
      }
    }
  }
}
